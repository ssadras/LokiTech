<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lokitech">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/lokitech_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/lokitech_logo"
        android:theme="@style/Theme.LokiTech">
        <activity
            android:name=".LockLogActivity"
            android:exported="false" />
        <activity
            android:name=".SetNewPinActivity"
            android:exported="false" />
        <activity
            android:name=".LockActivity"
            android:exported="false"
            android:parentActivityName=".LockListActivity" />
        <activity
            android:name=".LockListActivity"
            android:exported="false" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>